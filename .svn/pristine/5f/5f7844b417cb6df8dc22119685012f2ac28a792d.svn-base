<?xml version="1.0" encoding="utf-8" ?>

<Datatable xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xsi:noNamespaceSchemaLocation="../../Schemas/EncounterTargetingRangeDefinition.xsd">

    <!--Simulation Descriptors will be added to salvos upon being fired-->
    <EncounterTargetingRangeDefinition Name="Long" MinValue="20" MaxValue="60">
        <SimulationDescriptorReference Name="LongRange"/>
    </EncounterTargetingRangeDefinition>
    <EncounterTargetingRangeDefinition Name="Medium" MinValue="10" MaxValue="20">
        <SimulationDescriptorReference Name="MediumRange"/>
    </EncounterTargetingRangeDefinition>
    <EncounterTargetingRangeDefinition Name="Short" MinValue="1" MaxValue="10">
        <SimulationDescriptorReference Name="ShortRange"/>
    </EncounterTargetingRangeDefinition>
    <EncounterTargetingRangeDefinition Name="DeadZone" MaxValue="1">
        <SimulationDescriptorReference Name="DeadZoneRange"/>
    </EncounterTargetingRangeDefinition>

</Datatable>